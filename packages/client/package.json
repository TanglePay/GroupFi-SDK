  "dependencies": {
    "@iota/crypto.js": "2.0.0-rc.2",
    "@iota/iota.js": "2.0.0-rc.4",
    "@iota/pow-browser.js": "2.0.0-rc.3",
    "@iota/pow-node.js": "2.0.0-rc.3",
    "@iota/util.js": "2.0.0-rc.2",
    "big-integer": "^1.6.51",
