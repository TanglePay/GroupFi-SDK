  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "moduleNameIife": "groupfisdkclient",
  "scripts": {
    "clean": "rimraf dist",
    "build:type": "tsc",
    "build:source": "rollup --config rollup.config.mjs",
